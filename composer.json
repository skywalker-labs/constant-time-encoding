{
    "name": "skywalker-labs/constant-time-encoding",
    "type": "library",
    "description": "Constant-time implementations of RFC 4648 encoding (Base64, Base32, Hex) and other variants (Base64UrlSafe, Base64DotSlash, Base32Hex)",
    "keywords": [
        "base64",
        "encoding",
        "rfc4648",
        "base32",
        "base16",
        "hex",
        "bin2hex",
        "hex2bin",
        "base64_encode",
        "base64_decode",
        "base32_encode",
        "base32_decode"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Skywalker Labs Team",
            "email": "skywalkerlknw@gmail.com",
            "homepage": "https://skywalker-labs.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "infection/infection": "^0",
        "nikic/php-fuzzer": "^0",
        "phpunit/phpunit": "^9|^10|^11",
        "vimeo/psalm": "^4|^5|^6"
    },
    "autoload": {
        "psr-4": {
            "Skywalker\\ConstantTime\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skywalker\\ConstantTime\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs",
        "fix-style": "phpcbf",
        "mutation-test": "infection"
    },
    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "infection/extension-installer": true
        }
    },
    "support": {
        "issues": "https://github.com/skywalker-labs/constant-time-encoding/issues",
        "source": "https://github.com/skywalker-labs/constant-time-encoding"
    }
}